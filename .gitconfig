[alias]
	lg = ! git log --color --graph --pretty=format:'%Cred%h%Creset%C(yellow)%d%Creset %s %Cgreen%cr %Cblue%an%Creset' | less -r

[color]
	ui = true

[core]
	editor = vim

[achievement]
	upload = true

[init]
	defaultBranch = main

[commit]
	gpgsign = true

[gpg]
	program = gpg
[pull]
	ff = only
[filter "lfs"]
	process = git-lfs filter-process
	required = true
	clean = git-lfs clean -- %f
	smudge = git-lfs smudge -- %f
